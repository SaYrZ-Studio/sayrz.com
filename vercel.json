{
  "version": 2,
  "builds": [
    { "src": "src/index.js", "use": "@vercel/node" },
    { "src": "webroot/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/index.html", "dest": "/webroot/index.html" },
    { "src": "/(.*)", "dest": "/src/index.js" }
  ]
}
